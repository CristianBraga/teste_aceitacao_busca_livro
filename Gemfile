# fonte das gems, onde procurar por gems (repositorio global)
source 'https://rubygems.org'
# lista de gems utilizada no projeto
gem 'chromedriver-helper'
gem 'faker' # faker eh uma gem que gera dados aleatorios
gem 'page-object' # gem que implementa o padrao de "page objects"
gem 'pry'
gem 'require_all' # gem para carregar todos os arquivos de uma pasta
gem 'rspec-core' # "coracao" (funcionalidades principais) do rspec
gem 'rspec-expectations' # asserts do rspec, os "expects"
gem 'selenium-webdriver'
gem 'rake'
